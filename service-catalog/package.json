{
  "name": "@kyma-project/service-catalog-ui",
  "description": "Service Catalog UI in Kyma",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kyma-project/asyncapi-react": "^0.1.2",
    "@kyma-project/luigi-client": "^0.4.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.6",
    "react-app-rewired": "^2.0.2",
    "react-scripts": "2.1.3",
    "typescript": "3.2.4"
  },
  "scripts": {
    "start": "echo `` > public/config/config.js && PORT=8005 react-app-rewired start",
    "start:kyma": "npm run copy-config && BROWSER=none HOST=console-dev.kyma.local PORT=8005 react-app-rewired start",
    "start:kyma:api": "npm run copy-config && REACT_APP_LOCAL_API=true BROWSER=none HOST=console-dev.kyma.local PORT=8005 react-app-rewired start",
    "build": "REACT_APP_ENV=production react-app-rewired build",
    "test": "react-app-rewired test --passWithNoTests --env=jsdom",
    "eject": "react-app-rewired eject",
    "copy-config": "cp ../config.js public/config/config.js",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
