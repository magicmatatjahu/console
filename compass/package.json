{
  "name": "@kyma-project/compass-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kyma-project/luigi-core": "^0.4.12",
    "jsoneditor": "^6.2.1",
    "styled-components": "^3.4.10",
    "fiori-fundamentals": "^1.7.0",
    "fundamental-react": "^0.5.3",
    "fundamental-styles": "^0.1.0",
    "uuid": "^3.3.2",
    "uuid-validate": "0.0.3",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "start-live-server": "live-server --entry-file=index.html --mount=/luigi-core:./node_modules/@kyma-project/luigi-core public-luigi/",
    "start": "concurrently \"PORT=8888 ../node_modules/.bin/react-scripts start\" \"npm run buildConfig:watch\" \"wait-on http://localhost:8888/ && npm run start-live-server\" ",
    "start:kyma": "echo 'This component is not ready yet to be served together with Console.'",
    "start:kyma:api": "echo 'This component is not ready yet to be served together with Console.'",
    "build": "../node_modules/.bin/react-scripts build && ncp 'node_modules/@kyma-project/luigi-core' 'public-luigi/luigi-core'",
    "test": "../node_modules/.bin/react-scripts test",
    "eject": "../node_modules/.bin/react-scripts eject",
    "buildConfig": "webpack --mode production --config webpack-generateConfig.config",
    "buildConfig:watch": "webpack -d --config webpack-generateConfig.config --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "core-js": "^3.1.4",
    "ncp": "^2.0.0",
    "react-test-renderer": "^16.8.6",
    "waait": "^1.0.5",
    "live-server": "^1.2.1",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.3.5"
  },
  "config": {
    "unsafe-perm": true
  }
}
